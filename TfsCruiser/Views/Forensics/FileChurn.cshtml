@model Fenton.TeamServices.BuildRestApi.Churn
@{
    Layout = "~/Views/Shared/_layout.cshtml";
}

<ol class="churn">
    @foreach (var churn in Model.Files.Take(25))
    {
        <li title="@churn.ItemName (@churn.Count changes)" class="churn-@churn.Score">@churn.DisplayName<br />@churn.Count</li>
    }
</ol>